{
  "name": "font-switcher",
  "displayName": "Font Switcher",
  "description": "Quickly change your font from the command pallete.",
  "version": "3.1.0",
  "publisher": "evan-buss",
  "engines": {
    "vscode": "^1.22.0"
  },
  "author": {
    "name": "Evan Buss",
    "email": "evan.buss28@gmail.com",
    "url": "https://github.com/Evan-Buss"
  },
  "license": "WTFPL",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Evan-Buss/font-switcher"
  },
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:extension.switchFont",
    "onCommand:extension.setFontSize",
    "onCommand:extension.switchTerminalFont",
    "onCommand:extension.setTerminalFontSize"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.switchFont",
        "title": "Switch Font"
      },
      {
        "command": "extension.setFontSize",
        "title": "Font Size"
      },
      {
        "command": "extension.switchTerminalFont",
        "title": "Switch Terminal Font"
      },
      {
        "command": "extension.setTerminalFontSize",
        "title": "Terminal Font Size"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Font Switcher Configuration",
      "properties": {
        "font-switcher.enableLivePreview": {
          "type": "boolean",
          "default": false,
          "description": "Enable Font Switcher live preview when selecting font."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.10.6",
    "typescript": "^2.9.2",
    "@types/vscode": "^1.1.34",
    "glob": "^7.1.4"
  }
}
