{
	"name": "font-switcher",
	"displayName": "Font Switcher",
	"description": "Quickly change your font from the command pallete.",
	"version": "4.0.0",
	"publisher": "evan-buss",
	"engines": {
	  "vscode": "^1.55.0"
	},
	"author": {
	  "name": "Evan Buss",
	  "email": "evan.buss28@gmail.com",
	  "url": "https://github.com/Evan-Buss"
	},
	"license": "WTFPL",
	"categories": [
	  "Other"
	],
	"repository": {
	  "type": "git",
	  "url": "https://github.com/Evan-Buss/font-switcher"
	},
	"icon": "icon.png",
	"activationEvents": [
	  "onCommand:extension.switchFont",
	  "onCommand:extension.setFontSize",
	  "onCommand:extension.switchTerminalFont",
	  "onCommand:extension.setTerminalFontSize"
	],
	"main": "./out/extension",
	"contributes": {
	  "commands": [
		{
		  "command": "extension.switchFont",
		  "title": "Switch Font"
		},
		{
		  "command": "extension.setFontSize",
		  "title": "Font Size"
		},
		{
		  "command": "extension.switchTerminalFont",
		  "title": "Switch Terminal Font"
		},
		{
		  "command": "extension.setTerminalFontSize",
		  "title": "Terminal Font Size"
		}
	  ],
	  "configuration": {
		"type": "object",
		"title": "Font Switcher Configuration",
		"properties": {
		  "font-switcher.enableLivePreview": {
			"type": "boolean",
			"default": true,
			"description": "Enable Font Switcher live preview when selecting font."
		  }
		}
	  }
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
